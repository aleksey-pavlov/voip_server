            "type": "node"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${file}",
            "cwd": "${workspaceRoot}",
            "outFiles": [
                "${workspaceRoot}/build/**/*.js"
            ],
            "sourceMaps": true,
            "smartStep": true,

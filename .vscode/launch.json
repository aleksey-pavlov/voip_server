    "version": "0.2.0",
    "configurations": [
        {
            "args": [
                "-r",
                "ts-node/register",
                "--timeout",
                "999999",
                "${file}"
            ],
            "internalConsoleOptions": "openOnSessionStart",
            "name": "Mocha Tests",
            "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
            "request": "launch",
            "skipFiles": [

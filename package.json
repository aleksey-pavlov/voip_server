{
  "name": "diserver",
  "version": "1.6.7",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -r ./build",
    "setup": "node ./build/Migrations/DbInstall.js",
    "test": "mocha --exit -r ts-node/register src/**/*.test.ts",
    "server": "nodemon --inspect=0.0.0.0:9221 ./build/Server.js > logs/Server.log",
    "provider": "nodemon --inspect=0.0.0.0:9222 ./build/Provider.js > logs/Provider.log",
    "stat": "nodemon --inspect=0.0.0.0:9223 ./build/Stat.js > logs/Stat.log",
    "adminServer": "nodemon --inspect=0.0.0.0:9224 ./build/AdminServer.js > logs/AdminServer.log",
    "adminAgent": "nodemon --inspect=0.0.0.0:9225 ./build/AdminAgent.js > logs/AdminAgent.log",
    "dwgprototype": "nodemon --inspect=0.0.0.0:9226 ./build/DwgPrototype.js 8 8082 http://127.0.0.1:8171/591da31110a10c0021da097c > logs/DwgPrototype.log",
    "gateway": "nodemon --inspect=0.0.0.0:9227 ./build/Gateway.js --gatewayId 591da31110a10c0021da097c > logs/Gateway.log",
    "gatewayEventListener": "nodemon --inspect=0.0.0.0:9228 ./build/GatewayEventListener.js > logs/GatewayEventListener.log",
    "trunk": "nodemon --inspect=0.0.0.0:9229 ./build/Trunk.js --trunkId 5cd0a7744377b3001b79b99c > logs/Trunk.log",
    "trunkProvider": "nodemon --inspect=0.0.0.0:9230 ./build/TrunkProvider.js > logs/TrunkProvider.log",
    "asteriskAssetsManager": "nodemon --inspect=0.0.0.0:9231 ./build/AsteriskAssetsManager.js > logs/TrunkProvider.log",
    "debugAdmin": "npm run adminAgent | npm run adminServer",
    "debugMessages": "npm run provider | npm run server | npm run dwgprototype",
    "debugVoice": "npm run server | npm run trunkProvider | npm run stat",
    "debug": "npm run debugAdmin | npm run debugVoice | npm run debugMessages"
  },
  "author": "Aleksey Pavlov",
  "license": "ISC",

    "amqplib": "^0.5.2",
    "asterisk-manager": "^0.1.16",
    "body-parser": "^1.18.2",
    "csvtojson": "^2.0.8",
    "express": "^4.16.2",
    "fluentd-client": "^1.0.0",
    "md5": "^2.2.1",
    "moment": "^2.22.2",
    "mongodb": "^2.2.34",
    "mysql": "^2.15.0",
    "node-telegram-bot-api": "^0.64.0",
    "nodemailer": "^4.4.1",
    "pm2": "^2.9.1",
    "redis": "^2.8.0",
    "request": "^2.83.0",
    "smpp": "^0.3.1",
    "sox-stream": "^2.0.1",
